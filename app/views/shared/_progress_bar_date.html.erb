<div class="prog-bar-cont">
  <% if user_subject.user_end_date.present? %>
    <div class="arrow-down" id="arrow-real-<%= course_index %>-<%= index %>">
      <i class="fa fa-chevron-down" aria-hidden="true"></i>
    </div>
    <div id="point-real-<%= course_index %>-<%= index %>" class="date-on">
      <%= user_subject.user_end_date ? (l user_subject.user_end_date,
        format: :default) : t("user.no_start_date") %>
    </div>
  <% end %>
  <div class="prog-bar fix-size-bar">
    <div class="prog-bar-bg<%= check_on_time user_subject %>">
      <p></p>
    </div>
  </div>
  <div class="pull-left date-under" id="point-start-<%= course_index %>-<%= index %>">
    <%= user_subject.start_date ? (l user_subject.start_date,
      format: :default) : t("user.no_start_date") %>
  </div>
  <div class="pull-left date-under" id = "point-expect-<%= course_index %>-<%= index %>">
    <%= user_subject.end_date ? (l user_subject.end_date,
      format: :default) : t("user.no_start_date") %>
  </div>
  <div class="arrow-up">
    <i class="fa fa-chevron-up" aria-hidden="true"></i>
  </div>
  <div class="arrow-up" id="arrow-expect-<%= course_index %>-<%= index %>">
    <i class="fa fa-chevron-up" aria-hidden="true"></i>
  </div>

  <% if user_subject.user_end_date.present? %>
    <%= hidden_field_tag "spent_time"+course_index.to_s+index.to_s,
      (user_subject.user_end_date - user_subject.start_date).to_f %>
    <%= hidden_field_tag "expect_time"+course_index.to_s+index.to_s,
      (user_subject.end_date - user_subject.start_date).to_f %>
  <% end %>
</div>
